<%= form_with url: print_distribution_path(@distribution, format: :pdf), method: :get, class: "d-inline" do |f| %>
  <%= submit_button({ text: "Print", icon: "print", type: "outline-dark", size: "md" }) %>
  <%= check_box_tag :hide_values %>
  <%= f.label :values, "hide value columns on receipt" %>
  <%= check_box_tag :hide_packages %>
  <%= f.label :values, "hide packages column on receipt" %>
<% end %>
